{
    "rules": {
        "safe_walk_locations": {
            "$walkId": {
                ".read": "auth != null",
                ".write": "auth != null"
            }
        },
        "live_locations": {
            ".read": "auth != null",
            "$userId": {
                ".read": "auth != null",
                ".write": "auth != null && auth.uid == $userId"
            }
        }
    }
}